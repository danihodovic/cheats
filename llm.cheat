% llm

$ model: llm models list --- --column 2 --delimiter ':'

# Set default llm model
llm models default <model>

# list available models
llm models

# Install Mistral API models
llm install llm-mistral

# List LLM models on install
echo https://llm.datasette.io/en/stable/plugins/directory.html#remote-apis

# List LLM keys
llm keys list

# Set API key
llm keys set mistral
